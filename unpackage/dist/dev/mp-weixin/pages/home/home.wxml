<view class="homeBox"><block wx:if="{{loadstate==false}}"><view class="notload"><view class="loadback_box"><text class="loadback">返回</text></view><view class="title_img_box"><image class="title_img" src="../../static/zztj.png" mode></image></view><view class="username_box"><input class="username" placeholder="请输入用户名" type="text" id="{{plsuser}}" data-event-opts="{{[['input',[['__set_model',['','plsuser','$event',[]]]]]]}}" value="{{plsuser}}" bindinput="__e"/><image class="userimg" src="../../static/tabbar/my.png" mode></image></view><view class="userpas_box"><input class="userpas" placeholder="请输入密码" type="number" id="{{plspas}}" data-event-opts="{{[['input',[['__set_model',['','plspas','$event',[]]]]]]}}" value="{{plspas}}" bindinput="__e"/><image class="userpas_img" src="../../static/tabbar/pas.png" mode></image><text class="userpas_set">忘记密码</text></view><button type="primary" data-event-opts="{{[['tap',[['loadup',['$event']]]]]}}" bindtap="__e">登录</button><view class="loadchoose"><text data-event-opts="{{[['tap',[['toregister',['$event']]]]]}}" class="userset" bindtap="__e">注册账号</text><text data-event-opts="{{[['tap',[['nopasload',['$event']]]]]}}" class="nopasload" bindtap="__e">无密码登录</text></view><uni-popup class="loaded vue-ref" vue-id="1" data-ref="fail" bind:__l="__l" vue-slots="{{['default']}}">登录失败</uni-popup><uni-popup class="loaded vue-ref" vue-id="2" data-ref="suc" bind:__l="__l" vue-slots="{{['default']}}">登录成功</uni-popup></view></block><block wx:if="{{loadstate==true}}"><loadok vue-id="3" usersNickandId="{{usersNickandId}}" bind:__l="__l"></loadok></block><block wx:if="{{loadstate==true}}"><image class="user_set" src="../../static/set.png" data-event-opts="{{[['tap',[['setInfo',['$event']]]]]}}" bindtap="__e"></image></block><block wx:if="{{loadstate==true}}"><text data-event-opts="{{[['tap',[['logouted',['$event']]]]]}}" class="logout" bindtap="__e">退出登录</text></block><button type="primary" data-event-opts="{{[['tap',[['qwer',['$event']]]]]}}" bindtap="__e">asdfaf</button></view>